<div class="card">
  <div class="card-block">
    <h4 class="card-title"><%= user.username %></h4>
    <p class="card-text"><%= user.email %></p>
    <p class="card-text"><small class="text-muted">Last updated <%= distance_of_time_in_words(Time.now, user.updated_at) %> ago</small></p>
  </div>
</div>